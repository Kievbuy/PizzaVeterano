<h1>Your Oders</h1>

<br>

<table class="table table-striped table-bordered">
  <tr>
    <th>Order ID</th>
    <th>Include</th>
    <th>Name</th>
    <th>Phone</th>
    <th>Address</th>
    <th>Created at</th>
  </tr>

    <% @all_orders.each do |order| %>
      <tr>
        <td><%= order.id %></td>
        <td><%= order.orders_input %></td>
        <td><%= order.name %></td>
        <td><%= order.phone %></td>
        <td><%= order.address %></td>
        <td><%= order.created_at %></td>
      </tr>
    <% end %>

</table>
